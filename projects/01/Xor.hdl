/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    And(a=a, b=b, out=andAB);
    Not(in=andAB, out=notAndAB);

    Or(a=a, b=b, out=orAB);

    And(a=notAndAB, b=orAB, out=out);
}
